{
  "name": "superhabits",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel-watch -I -b env ./bin/www",
    "genSchema": "node_modules/.bin/babel db -d distdb --copy-files",
    "genMigration": "npm run genSchema && ts-node node_modules/.bin/typeorm migration:generate -n",
    "runMigrations": "ts-node node_modules/.bin/typeorm migration:run",
    "cleanDist": "rm -R distdb"

  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "~4.16.0",
    "googleapis": "^34.0.0",
    "hbs": "~4.0.1",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "pg": "^7.6.0",
    "redux-mock-store": "^1.5.3",
    "typeorm": "^0.2.8"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^2.0.7",
    "chai": "^4.2.0",
    "jsonwebtoken": "^8.3.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  }
}
